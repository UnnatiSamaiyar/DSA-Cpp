#include <bits/stdc++.h>
using namespace std;

int main(){
    int a0,a1,a2;
    cin>>a0>>a1>>a2;
    
    int b0,b1,b2;
    cin>>b0>>b1>>b2;
    
    int alice_count=0;
    int bob_count=0;
    
    if(a0>b0){
        alice_count++;
    }
    else if (b0>a0) {
        bob_count++;
    }
    
    if (a1>b1) {
        alice_count++;
    }
    else if (b1>a1) {
        bob_count++;
    }
    
    if(a2>b2){
        alice_count++;
    }
    else if(b2>a2){
        bob_count++;
    }
    
    cout<<alice_count<<" "<<bob_count;
}
