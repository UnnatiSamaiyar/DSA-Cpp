#include <bits/stdc++.h>
using namespace std;

int main(){
    int n;
    cin>>n;
    
    int arr[n];
    
    for(int i=0; i<n; i++){
        cin>>arr[i];
    }
    int biggest=0;
    for(int i=0; i<n; i++){
        biggest= max(biggest,arr[i]);
    }
    int count=0;
    for(int i=0; i<n; i++){
        if(arr[i]==biggest){
            count++;
            
        }
        cout<<count<<endl;
    }
    
}
