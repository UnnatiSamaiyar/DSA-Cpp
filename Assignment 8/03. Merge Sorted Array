class Solution {
public:
    void mergeArrays(int arr1[], int arr2[], int m, int n, int arr3[]){
        int i,j,k;
        while(i<m && j <m)
            if(arr1[i]<arr2[j])
                arr3[k++]=arr1[i++]
            else
                arr3[k++]=arr2[j++]
                
    }
    while (i<m);
        arr3[k++]=arr1[i++]
    while (i<n);
        arr3[k++]=arr2[j++]
        
    int main(){
        arr1=[1,2,5,6]
        arr2=[1,3,4,7]
        cout<<arr3
    }
};
